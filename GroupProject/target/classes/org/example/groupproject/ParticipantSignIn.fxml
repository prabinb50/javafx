<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane blendMode="MULTIPLY" prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #FFDD9A;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.groupproject.ParticipantLoginController">
    <children>
        <Label layoutX="153.0" layoutY="10.0" text="Education Feedback System" textFill="#f15a24">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <Label layoutX="193.0" layoutY="45.0" text="Welcome Back :)">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label layoutX="17.0" layoutY="75.0" text="To keep connected with us please login by using email or username and password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label layoutX="181.0" layoutY="115.0" text="Sign In as Participant" textFill="#f15a24">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label layoutX="166.0" layoutY="148.0" text="Email/Username" />
        <TextField fx:id="ParticipantEmail" layoutX="166.0" layoutY="169.0" prefHeight="25.0" prefWidth="170.0" />
        <Label layoutX="167.0" layoutY="206.0" text="Password" />
        <PasswordField fx:id="ParticipantPassword" layoutX="167.0" layoutY="226.0" prefHeight="25.0" prefWidth="170.0" />
        <Label layoutX="241.0" layoutY="255.0" text="Forget Password?" textFill="BLUE" />
        <Button layoutX="227.0" layoutY="289.0" mnemonicParsing="false" onAction="#buttonForParticipantLogin" style="-fx-background-color: blue;" text="Login" textFill="#fbf9f9" />
        <Label layoutX="148.0" layoutY="360.0" text="Don't have an account?">
            <font>
                <Font size="10.0" />
            </font>
        </Label>
        <Button layoutX="273.0" layoutY="357.0" mnemonicParsing="false" onAction="#buttonForParticipantSignUp" style="-fx-background-color: blue;" text="Sign Up as Participant" textFill="#fdfafa">
            <font>
                <Font size="10.0" />
            </font>
        </Button>
        <Label fx:id="LoginINFO" layoutX="267.0" layoutY="240.0" />
      <Button layoutX="405.0" layoutY="357.0" mnemonicParsing="false" onAction="#buttonForParticipantCancel" style="-fx-background-color: blue;" text="Cancel" textFill="#fdf9f9">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <ImageView blendMode="MULTIPLY" fitHeight="400.0" fitWidth="500.0" opacity="0.47" pickOnBounds="true">
         <image>
            <Image url="@../../../../login.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="20.0" fitWidth="21.0" layoutX="141.0" layoutY="173.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Group.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="15.0" fitWidth="21.0" layoutX="146.0" layoutY="232.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Vector%20(1).png" />
         </image>
      </ImageView>
    </children>


</AnchorPane>
